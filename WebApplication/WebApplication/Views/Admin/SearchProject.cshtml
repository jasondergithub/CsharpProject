@using WebApplication.Models;
@{ List<Project> projects123 = ViewBag.projects; }
<table class="table">
    <thead>
        <tr>
            <th>專案ID</th>
            <th>公司名稱</th>
            <th>專案內容</th>
            <th>創建者</th>
            <th>科別</th>
        </tr>
    </thead>
    <tbody>

        @foreach (Project project in projects123)
        {
<tr>
    <th>@project.projectId</th>
    <th>@project.companyName</th>
    <th>@project.usage</th>
    <th>@project.hospitalUser</th>
    <th>@project.department</th>
    <th>@Html.ActionLink("查看專案", "checkProject", new { projectId = project.projectId })</th>
</tr>
@*<th>
            @Html.ActionLink("編輯", "EditStaff", new { ID = staff.ID })
        @Html.ActionLink("刪除", "DeleteStaff", new { ID = staff.ID })
    </th>*@}

    </tbody>
</table>
